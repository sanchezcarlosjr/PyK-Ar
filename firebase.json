{
  "hosting": {
    "predeploy": "cd web && npm i && npm run build",
    "site": "pyk-ar",
    "public": "web/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {},
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  }
}
